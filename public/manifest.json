{
  "manifest_version": 2,
  "name": "Github Shortcuts",
  "version": "1.1.0",
  "description": "keyboard shortcuts for Github issue and pull request pages",
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "index.css"
      ]
    }
  ]
}
